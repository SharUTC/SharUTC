<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox xmlns:fx="http://javafx.com/fxml" alignment="CENTER_LEFT" fillWidth="true" layoutX="0.0" layoutY="0.0" prefHeight="80.0" prefWidth="180.0" styleClass="itemBox">
      <children>
        <Text fx:id="boxtitle" text="Titre" VBox.vgrow="ALWAYS">
          <fill>
            <Color blue="0.250" green="0.250" red="0.250" fx:id="x1"/>
          </fill>
          <font>
            <Font size="16.0" />
          </font>
          <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
          </VBox.margin>
        </Text>
        <Text fx:id="boxsubtitle" disable="false"  fill="$x1" text="Text">
          <font>
            <Font size="14.0" />
          </font>
          <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
          </VBox.margin>
        </Text>
      </children>
</VBox>
